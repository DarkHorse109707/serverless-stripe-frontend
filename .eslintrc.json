{
    "extends": "airbnb",
    "plugins": [
        "import"
    ],
    "rules": {
      "react/react-in-jsx-scope": 0,
      "react/jsx-filename-extension": 0,
      "no-console": 0
    },
    "globals": {
      "afterAll": true,
      "afterEach": true,
      "beforeAll": true,
      "beforeEach": true,
      "describe": true,
      "expect": true,
      "fail": true,
      "it": true,
      "jasmine": true,
      "jest": true,
      "test": true
    }
}
